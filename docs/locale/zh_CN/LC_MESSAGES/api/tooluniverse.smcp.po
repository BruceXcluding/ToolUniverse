msgstr ""

#: 65843758af164a0c88a6b2e33762ff74 of tooluniverse.smcp.SMCP.close:35
msgid "except KeyboardInterrupt:"
msgstr ""

#: 6d1649920a3d4b43b2271cc89787a99b of tooluniverse.smcp.SMCP.close:36
msgid "pass  # run_simple() handles cleanup"
msgstr ""

#: b2c5243565af4a2287e358706460be21 of tooluniverse.smcp.SMCP.close:39
